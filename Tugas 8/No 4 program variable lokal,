#include <stdio.h>

int i = 2;  // Inisialisasi variabel global i

int main() {
    printf("nilai i didalam main() = %d\n", i);

    fungsi1();  // Memanggil fungsi1

    return 0;
}

void fungsi1(void) {
    int i = 5;
    
    // Menambahkan program untuk menghasilkan angka 10
    i = i + 5;

    printf("nilai i didalam fungsi1() = %d\n", i);
}
